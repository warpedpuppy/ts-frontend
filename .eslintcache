<section className=\"hsl-controls\">\n            <fieldset><legend>hue</legend>\n                <div className=\"hue-spinner\" style={hueStyle}>\n                    <div></div>\n                </div>\n                <div>{this.state.hueRotation}</div>  \n            </fieldset>\n            <fieldset><legend>saturation</legend>\n                <div className=\"saturation-bar\">\n                    <div style={saturationStyle}></div>\n                </div>\n                <div>{this.state.saturation}</div>  \n            </fieldset>\n            <fieldset><legend>lightness</legend>\n                <div className=\"saturation-bar\">\n                    <div style={lightnessStyle}></div>\n                </div>\n                <div>{Math.floor(this.state.lightness)}</div>  \n            </fieldset>\n        </section>\n        <div className=\"color-result\" style={colorProduct}></div>\n      </div>\n    );\n  }\n}\n","/Users/edwardwalther/Sites/tryingsomething/frontend/src/pages/css/CSS.js",[],"/Users/edwardwalther/Sites/tryingsomething/frontend/src/pages/css/color/rgb/RGB.js",[],"/Users/edwardwalther/Sites/tryingsomething/frontend/src/pages/css/color/rgb/RBGSlider.js",["89"],"import React from 'react'\n\nexport default function RBGSlider(props) {\n    return (\n        <select value={props.color} onChange={(e) => props.changeColor(props.id, e.target.value)}>\n        {\n            props.arr.map( (item, index) => {\n                let selected = item === props.color ? true : false;\n                return <option value={item} key={index}>{item}</option>\n            })\n        }\n        </select>\n    )\n}\n",{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","severity":1,"message":"95","line":3,"column":8,"nodeType":"96","messageId":"97","endLine":3,"endColumn":15},{"ruleId":"94","severity":1,"message":"98","line":9,"column":8,"nodeType":"96","messageId":"97","endLine":9,"endColumn":19},{"ruleId":"94","severity":1,"mess